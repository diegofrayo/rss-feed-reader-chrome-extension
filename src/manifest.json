{
	"manifest_version": 2,
	"name": "RSS Reader",
	"description": "This extension allows add and read RSS Channels",
	"version": "1.0",
	"browser_action": {
		"default_icon": "img/icon.png",
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"bookmarks",
		"https://ajax.googleapis.com/"
	]
}