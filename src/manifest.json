{
	"manifest_version": 2,
	"name": "RSS Feed Reader",
	"description": "RSS Feed Reader",
	"version": "1.0",
	"browser_action": {
		"default_icon": "img/icon-128.png",
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"bookmarks",
		"tabs",
		"https://ajax.googleapis.com/"
	],
	"icons": {
		"16": "img/icon-16.png",
		"48": "img/icon-48.png",
		"128": "img/icon-128.png"
	}
}